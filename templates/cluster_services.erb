<% if @services.empty? %>
There are no services.
<% else %>
<% @services.each do |service, tasks| %>
<% tasks.sort.each do |task| %>
--------------------------------------------------------
<% service = service.match(/\//)%>
<% task = task.match(/\//)%>
service_name:  <%= "#{service.post_match}" %>
attached_task: <%= "#{task.post_match}" %>
--------------------------------------------------------
<% end %>
<% end %>
<% end %>
